---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/">
	<channel>
		<title>Chris Johnson's Blog Export Feed</title>
		<description></description>		
		<link>{{ site.url }}</link>
		<atom:link href="{{ site.url }}/blog/export_feed.xml" rel="self" type="application/rss+xml" />
		{% for post in site.posts limit:999 %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<description>{{ post.raw_content | xml_escape }}</description>
				<pubDate>{{ post.date | date: "%a, %d %b %Y 11:00:00 -0500" }}</pubDate>
				<rawDate>{{ post.date }}</rawDate>
				<link>{{ post.url }}</link>
				<guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>